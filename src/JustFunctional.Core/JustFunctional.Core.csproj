<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <Nullable>Enable</Nullable>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>JustFunctional.Core</PackageId>
    <Authors>Jose Carlos Nunez Martinez</Authors>
    <Company>SimpleSolutions</Company>
    <Description>An easy-to-use, customizable math evaluator</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://dominioncfg.github.io/just-functional-read-the-docs/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dominioncfg/just-functional</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>Math;Evaluator</PackageTags>
  </PropertyGroup>
</Project>
